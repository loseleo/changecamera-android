<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DdPagerView">
        <attr name="pageview_orientation" format="integer">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="DashView">
        <attr name="dashWidth" format="dimension"/>
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="lineColor" format="color"/>
        <attr name="dashOrientation" format="integer"/>
    </declare-styleable>
    <declare-styleable name="FilletBtView">
        <attr name="fillet" format="dimension" />
        <attr name="defaultColor" format="color" />
        <attr name="pressedColor" format="color" />
        <attr name="disabledColor" format="color" />
    </declare-styleable>

</resources>
